<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	<g:SplitLayoutPanel ui:field="topLayoutPanel">
		<g:north size="100">
			<g:SimplePanel ui:field="header"></g:SimplePanel>
		</g:north>
		<g:west size="100">
			<g:SimplePanel ui:field="sidebar"></g:SimplePanel>
		</g:west>
		<g:center>
			<g:SimplePanel ui:field="content"></g:SimplePanel>
		</g:center>
		<g:south size="100">
			<g:SimplePanel ui:field="footer"></g:SimplePanel>
		</g:south>
	</g:SplitLayoutPanel>
</ui:UiBinder>