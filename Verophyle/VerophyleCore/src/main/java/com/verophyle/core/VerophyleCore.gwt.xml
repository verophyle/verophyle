<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to='VerophyleCore'>
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.i18n.I18N"/>
  <inherits name="com.google.gwt.activity.Activity"/>
  <inherits name="com.google.gwt.inject.Inject"/>
  <inherits name="com.google.gwt.resources.Resources" />
  <inherits name="com.googlecode.objectify.Objectify" />
  <inherits name='com.google.web.bindery.requestfactory.RequestFactory' />

  <extend-property name="locale" values="en"/>
  <extend-property name="locale" values="fr"/>
  <set-property-fallback name="locale" value="en"/>
  
  <entry-point class='com.verophyle.core.client.CoreEntryPoint'/>

  <source path='client'/>
  <source path='shared'/>
  
  <stylesheet src="http://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600%26subset=latin,latin-ext"/>
</module>
